{
  "expo": {
    "name": "Xzero Partner",
    "slug": "Xzero-Partner",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": ["ios", "android"],
    "splash": {
      "image": "./assets/splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "enabled": false
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "icon": "./assets/icon_ios.png",
      "supportsTablet": true,
      "bundleIdentifier": "com.xzero.partner",
      "usesAppleSignIn": true,
      "infoPlist":{
        "CFBundleShortVersionString": "0.0.1"
      }
    },
    "android":{
      "package": "com.xzero.partner",
      "icon": "./assets/icon.png",
      "permissions": [],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
